class Time
{
    int minutes
    int hours
    
    Time plus(Time t)
    {    Time newTime=new Time()
        newTime.minutes=this.minutes+t.minutes
        newTime.hours=this.hours+t.hours
        
        return newTime
    }
    String toString()
    {
        "${hours}:${minutes}"
    }
}
Time time1=new Time(hours:3,minutes:20)
Time time2=new Time(hours:3,minutes:20)
print time1 + time2